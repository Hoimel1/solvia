title = Energy minimization
integrator = steep
nsteps = 5000
emtol = 1000.0
emstep = 0.01

nstlist = 10
cutoff-scheme = Verlet
ns_type = grid
pbc = xyz
verlet-buffer-tolerance = 0.005

coulombtype = reaction-field
rcoulomb = 1.1
epsilon_r = 15
epsilon_rf = 0
vdw_type = cutoff
vdw-modifier = Potential-shift-verlet
rvdw = 1.1

constraints = none
